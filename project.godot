config_version=5

[application]

config/name="Tabletop Framework"
config/description="A 2-dimensional virtual tabletop for all! Create board game configurations which can be loaded in and played with friends online or in person!"
config/version="0.0.1"
run/main_scene="res://src/scenes/menu/main_menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.164706, 0.172549, 0.192157, 1)
boot_splash/image="res://assets/banners/banner.png"
config/icon="res://assets/icons/icon_transparent.png"
config/windows_native_icon="res://assets/icons/Windows/icon_windows.ico"

[autoload]

Globals="*res://src/scripts/singletons/globals.gd"
Utils="*res://src/scripts/singletons/utils.gd"
SignalManager="*res://src/scripts/singletons/signal_manager.gd"
GameProperties="*res://src/scripts/singletons/game_properties.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1280
window/size/viewport_height=1280
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=6
window/ios/allow_high_refresh_rate=false
window/ios/hide_status_bar=false

[filesystem]

import/blender/enabled=false

